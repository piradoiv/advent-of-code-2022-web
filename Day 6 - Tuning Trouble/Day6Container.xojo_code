		End Sub
	#tag EndEvent
#tag EndEvents
#tag Events PuzzleInputDialog
	#tag Event
		Sub InputReady(inputValue As String)
		  Var marker1 As Integer = GetMessageMarker(inputValue, 4)
		  Var marker2 As Integer = GetMessageMarker(inputValue, 14)
		  
		  Part1TextField.Text = marker1.ToString
		  Part2TextField.Text = marker2.ToString
		  
		  mResult = inputValue.Middle(marker2, 4)
		  Flipboard.Word = mResult
		  
		  Var rows() As String
		  For i As Integer = 0 To inputValue.Length - 4
		    rows.Add(inputValue.Middle(i, 4).Uppercase)
		  Next
		  
		  DataSource.ValidResult = mResult
		  DataSource.Configurations = rows
		  AttemptsListBox.ReloadData
		End Sub
	#tag EndEvent
	#tag Event
		Function RealInputDataRequested() As String
		  Return kRealData
		End Function
	#tag EndEvent
	#tag Event
		Function TestInputDataRequested() As String
		  Return kTestData
		End Function
	#tag EndEvent
#tag EndEvents
#tag Events AttemptsListBox
	#tag Event
		Sub Opening()
		  Me.DataSource = DataSource
		End Sub
	#tag EndEvent
	#tag Event
		Sub SelectionChanged(Rows() as Integer)
		  If Rows.Count = 0 Then
		    Flipboard.Word = "----"
		  End If
		  
		  Flipboard.Word = Me.SelectedRowValue
		End Sub
	#tag EndEvent
	#tag Event
		Sub Pressed(row as integer, column as integer)
		  Flipboard.Word = DataSource.Configurations(row)
		End Sub
	#tag EndEvent
#tag EndEvents
#tag Events Flipboard
	#tag Event
		Sub CharIsBeingDisplayed(position As Integer, char As String)
		  Var statuses() As WebRectangle = Array(StatusRectangle1, StatusRectangle2, StatusRectangle3, StatusRectangle4)
		  
		  Var resultChars() As String = mResult.Split("")
		  Var valid As Boolean = resultChars.LastIndex >= position And resultChars(position) = char
		  statuses(position).BackgroundColor = If(valid, Color.Green, Color.Red)
		End Sub
	#tag EndEvent
#tag EndEvents
