#tag WindowCode
	#tag Event
		Sub Shown()
		  Part1Tests(kTestData)
		  Part2Tests(kTestData)
		End Sub
	#tag EndEvent


	#tag Method, Flags = &h21
		Private Function GetMessageMarker(data As String, amount As Integer) As Integer
		  For i As Integer = 0 To data.Length - 1
		    Var set As New Dictionary
		    Var chars() As String = data.Middle(i, amount).Split("")
		    
		    For Each char As String In chars
		      set.Value(char) = True
		    Next
		    
		    If set.KeyCount = amount Then Return i + amount
		  Next
		End Function
	#tag EndMethod

	#tag Method, Flags = &h21
		Private Sub Part1Tests(value As String)
		  Var examples() As Pair = Array("mjqjpqmgbljsphdztnvjfqwrcgsmlb" : 7, _
		  "bvwbjplbgvbhsrlpgdmjqwftvncz" : 5, _
		  "nppdvjthqldpwncqszvftbrmjlhg" : 6, _
		  "nznrnfrfntjfmvfwmzdfjlvtqnbhcprsg" : 10, _
		  "zcfzfwzzqfrljwzlrfnpqdbhtmscgvjw" : 11)
		  
		  Var success As Boolean = True
		  For Each example As Pair In examples
		    Var result As Integer = GetMessageMarker(example.Left, 4)
		    If result <> example.Right Then
		      System.DebugLog("Expected '" + example.Right + "', got: '" + result.ToString + "'")
		      success = False
		    End If
		  Next
		  
		  If Not success Then
		    MessageBox("Tests failed, check debug log")
		  End If
		End Sub
	#tag EndMethod

	#tag Method, Flags = &h21
		Private Sub Part2Tests(value As String)
		  Var examples() As Pair = Array("mjqjpqmgbljsphdztnvjfqwrcgsmlb" : 19, _
		  "bvwbjplbgvbhsrlpgdmjqwftvncz" : 23, _
		  "nppdvjthqldpwncqszvftbrmjlhg" : 23, _
		  "nznrnfrfntjfmvfwmzdfjlvtqnbhcprsg" : 29, _
		  "zcfzfwzzqfrljwzlrfnpqdbhtmscgvjw" : 26)
		  
		  Var success As Boolean = True
		  For Each example As Pair In examples
		    Var result As Integer = GetMessageMarker(example.Left, 14)
		    If result <> example.Right Then
